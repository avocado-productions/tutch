{
    "name": "tutch",
    "version": "0.1.1",
    "description": "Typescript implementation of Tutch, a TUTorial proof CHecker.",
    "main": "dist/index.js",
    "types": "dist/index.d.js",
    "files": [
        "/dist/**/*"
    ],
    "scripts": {
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "test": "nyc mocha -r ts-node/register src/**/*_test.ts",
        "prenearley": "mkdirp dist",
        "nearley:spec": "nearleyc tests/testspec.ne -o dist/spec-rules.js",
        "nearley:tutch": "nearleyc syntax/proof.ne -o dist/rules.js",
        "nearley:proposition": "nearleyc syntax/proposition.ne -o dist/proposition.js",
        "nearley": "npm-run-all --parallel nearley:spec nearley:tutch nearley:proposition",
        "prettier": "prettier --write **/*.json src/*.ts src/**/*.ts",
        "tsc": "tsc",
        "watch": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrisamaphone/tutch.git"
    },
    "author": "Chris Martens and Rob Simmons",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/chrisamaphone/tutch/issues"
    },
    "homepage": "https://github.com/chrisamaphone/tutch#readme",
    "devDependencies": {
        "@calculemus/impossible": "0.0.4",
        "@types/chai": "^4.2.1",
        "@types/mocha": "^5.2.6",
        "@types/moo": "^0.5.0",
        "@types/node": "^12.7.4",
        "chai": "^4.2.0",
        "coveralls": "^3.0.3",
        "mkdirp": "^0.5.1",
        "mocha": "^6.1.3",
        "npm-run-all": "^4.1.5",
        "nyc": "^14.1.1",
        "prettier": "^1.17.0",
        "travis": "^0.1.1",
        "ts-node": "^8.0.3",
        "tslint": "^5.19.0",
        "typescript": "^3.6.2",
        "webpack": "^4.39.3"
    },
    "dependencies": {
        "@types/nearley": "^2.11.0",
        "moo": "^0.5.0",
        "nearley": "^2.19.0"
    },
    "nyc": {
        "include": [
            "dist/**/*.js",
            "src/**/*.ts"
        ],
        "exclude": [
            "src/**/*.d.ts",
            "src/**/*_test.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "sourcemap": true,
        "instrument": true
    },
    "prettier": {
        "printWidth": 110,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5"
    }
}
